Include /Users/mbarkan/.colima/ssh_config

Host *
   AddKeysToAgent yes
   UseKeychain yes
   ForwardAgent yes

Host bastion
   HostName bastion.inside-box.net
   User mbarkan
   ForwardAgent yes

Host appconf-push
    HostName appconf-push
    ProxyJump bastion
    ForwardAgent yes

Host gcp-skynet
   HostName gcp-skynet-util-prod
   ProxyJump bastion 

Host dev
   HostName gcp-skynet-util-dev

Host dev-pod
   HostName mbarkan.inside-box.net
   User mbarkan
   ForwardAgent yes

Host docker162
  HostName 127.0.0.1
  Port 16222
  User workspace
